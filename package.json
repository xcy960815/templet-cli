{
    "name": "templet-cli",
    "version": "0.1.3",
    "description": "模板脚手架,内置各种vue模板、rollup-js、rollup-ts、rollup-vue-js、rollup-vue-ts、vscode-webview-ts构建模板，无需配置,一键配置使用",
    "type": "commonjs",
    "bin": {
        "tem": "/bin/index.js"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ENV=daily  node ./init-config.js",
        "upload": "cross-env NODE_ENV=production ENV=prod node ./init-config.js"
    },
    "repository": {
        "type": "git",
        "url": " https://github.com/ChongYu-Yease/mini-cli.git"
    },
    "keywords": [
        "cli",
        "vue-template",
        "mini-cli",
        "rollup-template",
        "rollup-vue-template",
        "vsdoe-webview-template"
    ],
    "author": "chongyu",
    "license": "MIT",
    "engines": {
        "node": "^12.0.0 || >= 14.0.0"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "co": "^4.6.0",
        "co-prompt": "^1.0.0",
        "commander": "^8.1.0",
        "cross-env": "^7.0.3",
        "download-git-repo": "^3.0.2",
        "execa": "^5.1.1",
        "handlebars": "^4.7.7",
        "inquirer": "^8.1.2",
        "internet-available": "^1.0.0",
        "is-online": "^9.0.1",
        "log-symbols": "^5.0.0",
        "ora": "^5.4.1",
        "progress": "^2.0.3",
        "request": "^2.88.2",
        "semver": "^7.3.5",
        "single-line-log": "^1.1.2"
    }
}